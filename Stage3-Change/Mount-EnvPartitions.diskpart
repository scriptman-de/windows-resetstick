SELECT DISK 0

REM BOOT
SELECT PARTITION 1
REMOVE ALL
ASSIGN LETTER=S

REM ResetOS
SELECT PARTITION 3
GPT ATTRIBUTES=0x0000000000000000
REMOVE ALL
ASSIGN LETTER=R

REM PageFile
SELECT PARTITION 4
REMOVE ALL
ASSIGN LETTER=P

REM Images
SELECT PARTITION 5
GPT ATTRIBUTES=0x0000000000000000
REMOVE ALL
ASSIGN LETTER=W


LIST VOLUME